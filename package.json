{
  "name": "sidecar",
  "version": "0.0.0",
  "description": "Sidecar module for Moleculer microservices framework with HTTP support.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "test": "jest --runInBand --coverage",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moleculerjs/sidecar.git"
  },
  "keywords": [
    "moleculer",
    "microservices",
    "sidecar"
  ],
  "author": "MoleculerJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moleculerjs/sidecar/issues"
  },
  "homepage": "https://github.com/moleculerjs/sidecar#readme",
  "devDependencies": {
    "benchmarkify": "^2.1.3",
    "coveralls": "^3.1.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "dependencies": {
    "fastest-validator": "^1.9.0",
    "kleur": "^4.1.3",
    "lodash": "^4.17.20",
    "moleculer": "^0.14.0",
    "moleculer-repl": "^0.6.4",
    "moleculer-web": "^0.10.0-beta1",
    "node-fetch": "^2.6.1"
  }
}
